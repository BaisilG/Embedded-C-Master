int main(void)
{
	float num1, num2, num3;
	float average;

	printf("Enter the first number:");
	scanf("%f", &num1);

	printf("Enter the second number:");
		scanf("%f", &num2);

	printf("Enter the third number:");
			scanf("%f", &num3);


	average = (num1+num2+num3);

}
















































































































